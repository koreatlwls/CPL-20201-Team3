<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MyPage">
    <ImageView
        android:id="@+id/img_user"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/btn_star"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/text_parttime"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/text_saved_parttime"
        app:layout_constraintTop_toBottomOf="@id/btn_parttime"
        android:text="아르바이트 신청" />

    <ImageButton
        android:id="@+id/btn_parttime"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:src="@android:drawable/btn_star"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/btn_blue"
        app:layout_constraintRight_toLeftOf="@id/btn_saved_parttime"
        app:layout_constraintTop_toBottomOf="@id/img_user" />

    <TextView
        android:id="@+id/text_saved_parttime"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/btn_parttime"
        app:layout_constraintLeft_toRightOf="@id/text_parttime"
        app:layout_constraintRight_toLeftOf="@id/text_saved_AD"
        android:text="아르바이트 내역" />
    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toRightOf="@id/btn_parttime"
        app:layout_constraintTop_toTopOf="@id/btn_parttime"
        android:src="@android:drawable/btn_star"
        app:layout_constraintRight_toLeftOf="@id/btn_saved_AD"
        android:background="@drawable/btn_blue"
        android:id="@+id/btn_saved_parttime"/>

    <TextView
        android:id="@+id/text_saved_AD"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/text_saved_parttime"
        app:layout_constraintTop_toBottomOf="@id/btn_parttime"
        android:text="채용 내역" />
    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toRightOf="@id/btn_saved_parttime"
        app:layout_constraintTop_toTopOf="@id/btn_saved_parttime"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/btn_blue"
        android:src="@android:drawable/btn_star"
        android:id="@+id/btn_saved_AD"
        />
    <TextView
        android:id="@+id/text_request"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/text_saved_parttime"
        app:layout_constraintTop_toBottomOf="@id/btn_request"
        android:text="통합회원 신청" />

    <ImageButton
        android:id="@+id/btn_request"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:src="@android:drawable/btn_star"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/btn_blue"
        app:layout_constraintRight_toLeftOf="@id/btn_saved_parttime"
        app:layout_constraintTop_toBottomOf="@id/btn_parttime" />

    <TextView
        android:id="@+id/text_ing"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/btn_ing"
        app:layout_constraintLeft_toRightOf="@id/text_parttime"
        app:layout_constraintRight_toLeftOf="@id/text_saved_AD"
        android:text="진행중인 알바" />
    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toRightOf="@id/btn_parttime"
        app:layout_constraintTop_toTopOf="@id/btn_request"
        android:src="@android:drawable/btn_star"
        app:layout_constraintRight_toLeftOf="@id/btn_saved_AD"
        android:background="@drawable/btn_blue"
        android:id="@+id/btn_ing"/>

    <TextView
        android:id="@+id/text_modify"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/text_saved_parttime"
        app:layout_constraintTop_toBottomOf="@id/btn_modify"
        android:text="회원정보 수정" />
    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintLeft_toRightOf="@id/btn_ing"
        app:layout_constraintTop_toTopOf="@id/btn_request"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/btn_blue"
        android:src="@android:drawable/btn_star"
        android:id="@+id/btn_modify"
        />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEST NAME"
        app:layout_constraintLeft_toRightOf="@id/img_user"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toTopOf="@id/img_user"
        android:layout_marginTop="20dp"
        />

    <ImageView
        android:id="@+id/star1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/star_on"
        app:layout_constraintLeft_toLeftOf="@id/name"
        app:layout_constraintRight_toLeftOf="@id/star2"
        app:layout_constraintTop_toBottomOf="@id/email"
        />

    <ImageView
        android:id="@+id/star2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/star_on"
        app:layout_constraintLeft_toRightOf="@id/star1"
        app:layout_constraintRight_toLeftOf="@id/star3"
        app:layout_constraintTop_toBottomOf="@id/email"
        />

    <ImageView
        android:id="@+id/star3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/star_on"
        app:layout_constraintLeft_toRightOf="@id/star2"
        app:layout_constraintRight_toLeftOf="@id/star4"
        app:layout_constraintTop_toBottomOf="@id/email"
        />

    <ImageView
        android:id="@+id/star4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/star_on"
        app:layout_constraintLeft_toRightOf="@id/star3"
        app:layout_constraintRight_toLeftOf="@id/star5"
        app:layout_constraintTop_toBottomOf="@id/email"
        />

    <ImageView
        android:id="@+id/star5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/star_on"
        app:layout_constraintLeft_toRightOf="@id/star4"
        app:layout_constraintTop_toBottomOf="@id/email"
        />

    <EditText
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:text="as00098@naver.com"
        android:inputType="textEmailAddress"
        app:layout_constraintTop_toBottomOf="@id/name"
        app:layout_constraintLeft_toLeftOf="@id/name"
        />



</androidx.constraintlayout.widget.ConstraintLayout>